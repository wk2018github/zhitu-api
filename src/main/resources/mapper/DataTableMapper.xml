<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="zhitu.sq.dataset.mapper.DataTableMapper" >
 
 <select id="findByDataSetId" resultType="java.util.Map" statementType="STATEMENT">
 	select * from ${tableName}
 </select>
 <delete id="deleteByDataSetId" parameterType="java.lang.String" statementType="STATEMENT">
 	DROP TABLE IF EXISTS ${tableName}
 </delete>
 <select id="findByTableAndId" resultType="java.util.Map" parameterType="java.lang.String">
 	select * from ${tableName} where id = #{id}
 </select>
</mapper>